package com.inn.observablepattern;

public class NewsChannel implements Channels {
	
	String news;

	@Override
	public void update(String news) {
		// TODO Auto-generated method stub
		this.setNews(news);
	}
	
	void setNews(String news) {
		this.news=news;
	}

	@Override
	public String getNews() {
		// TODO Auto-generated method stub
		return this.news;
	}
}
