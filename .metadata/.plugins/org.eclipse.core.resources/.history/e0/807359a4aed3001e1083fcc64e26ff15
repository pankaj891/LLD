package org.example;

import java.util.Comparator;

import org.example.Main.pair;

public class IntervalSorting implements Comparator<Main.pair>{

	@Override
	public int compare(pair o1, pair o2) {
		if (o1.a<o2.a || (o1.a==o2.a && o1.a<o2.b))
			return 1;
		else if (o1.a==o2.a && o1.b==o2.b)
			return 0;
		return -1;
	}

}
